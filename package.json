{
  "name": "notion-export-mcp-server",
  "version": "0.0.3",
  "description": "A MCP(Model Context Protocol) server that accesses to Notion unofficial export API",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "notion-export-mcp-server": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "node --import ./ts-node-loader.js src/index.ts",
    "build": "tsc -p tsconfig.build.json && shx chmod +x dist/*.js",
    "start": "node dist/index.js",
    "test": "echo \"No tests yet\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"src/**/*.ts\" \"examples/**/*.ts\"",
    "lint:prettier": "prettier --check \"src/**/*.ts\" \"examples/**/*.ts\"",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint \"src/**/*.ts\" \"examples/**/*.ts\" --fix",
    "fix:prettier": "prettier --write \"src/**/*.ts\" \"examples/**/*.ts\"",
    "examples": "node --import ./ts-node-loader.js examples/export_page.ts",
    "examples:write": "WRITE_TO_FILE=true OUTPUT_BASE_PATH=./out node --import ./ts-node-loader.js examples/export_page.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syucream/notion-export-mcp-server.git"
  },
  "keywords": [
    "notion",
    "mcp"
  ],
  "author": "syucream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syucream/notion-export-mcp-server/issues"
  },
  "homepage": "https://github.com/syucream/notion-export-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@types/node": "^20.10.3",
    "dotenv": "^16.4.7",
    "adm-zip": "^0.5.10",
    "axios": "^1.6.2",
    "typescript": "^5.3.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2"
  }
}
